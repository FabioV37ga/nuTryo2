{
  "name": "nutryo",
  "version": "1.0.0",
  "description": "",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "start:front": "npm run --prefix frontend dev",
    "start:back": "npm run dev:back && npm run server:back",
    "start:api": "npm run build:api && npm run dev:api",
    "build:front": "npm run --prefix frontend build",
    "build:api": "tsc -p api/tsconfig.json",
    "preview:front": "npm run --prefix frontend preview",
    "server:back": "nodemon backend/dist/server.js",
    "server:back:prod": "node backend/dist/server.js",
    "dev:back": "tsc -p backend/tsconfig.json",
    "dev:api": "nodemon api/dist/server.js",
    "dev:api:prod": "node api/dist/server.js",
    "clearback": "powershell -Command \"Get-ChildItem -Path backend/dist | Remove-Item -Recurse -Force\"",
    "clearapi": "powershell -Command \"Get-ChildItem -Path api/dist | Remove-Item -Recurse -Force\"",
    "prod:front": "npm run build:front && npm run preview:front",
    "prod:back": "npm run dev:back && npm run server:back:prod",
    "prod:api": "npm run build:api && npm run dev:api:prod",
    "--- LINUX/DOCKER VERSÃ•ES ---": "",
    "clearback:unix": "shx rm -rf backend/dist/*",
    "clearapi:unix": "shx rm -rf api/dist/*",
    "prod:front:unix": "npm run build:front && npm run preview:front",
    "prod:back:unix": "npm run dev:back && npm run server:back:prod",
    "prod:api:unix": "npm run build:api && npm run dev:api:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "lite-server": "^2.6.1",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "nodemon": "^3.1.10",
    "pdf-lib": "^1.17.1",
    "typescript": "^5.9.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/xlsx": "^0.0.36",
    "chokidar-cli": "^3.0.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4"
  }
}
