  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "lite-server --baseDir=frontend/dist",
    "start": "npm-run-all copyToDist:frontend dev server",
    "dev": "tsc -p frontend/tsconfig.json",
    "watch": "tsc -w -p frontend/tsconfig.json",
    "copyToDist:frontend": "xcopy frontend\\src\\images frontend\\dist\\images /E /I /Y && xcopy frontend\\src\\styles frontend\\dist\\styles /E /I /Y && copy frontend\\src\\index.html frontend\\dist\\index.html /Y",
    "clearfront": "powershell -Command \"Remove-Item -Recurse -Force frontend/dist/*\""
  },
