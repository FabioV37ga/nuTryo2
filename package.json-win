{
  "name": "nutryo",
  "version": "1.0.0",
  "description": "",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "start:front": "npm run copyToDist:frontend && npm run dev:front && npm-run-all --parallel watch:sync server:front",
    "start:back": "npm run dev:back && npm run server:back",
    "server:front": "lite-server --baseDir=frontend/dist",
    "server:back": "nodemon backend/dist/server.js",
    "server:back:prod": "node backend/dist/server.js",
    "dev:front": "tsc -p frontend/tsconfig.json",
    "dev:back": "tsc -p backend/tsconfig.json",
    "watch": "tsc -w -p frontend/tsconfig.json",
    "copyToDist:frontend": "xcopy frontend\\src\\images frontend\\dist\\images /E /I /Y && xcopy frontend\\src\\styles frontend\\dist\\styles /E /I /Y && copy frontend\\src\\index.html frontend\\dist\\index.html /Y",
    "sync": "xcopy frontend\\dist\\index.html frontend\\src\\index.html /Y && xcopy frontend\\dist\\styles frontend\\src\\styles /E /I /Y && xcopy frontend\\dist\\images frontend\\src\\images /E /I /Y",
    "watch:sync": "chokidar \"frontend/dist/index.html\" \"frontend/dist/styles/**/*\" \"frontend/dist/images/**/*\" -c \"npm run sync\"",
    "clearfront": "powershell -Command \"Get-ChildItem -Path frontend/dist | Remove-Item -Recurse -Force\"",
    "prod:front":"npm run copyToDist:frontend && npm run dev:front && npm run server:front",
    "prod:back":"npm run dev:back && npm run server:back:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "lite-server": "^2.6.1",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/xlsx": "^0.0.36",
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5"
  }
}
